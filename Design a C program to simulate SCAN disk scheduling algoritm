    printf("Enter the size of queue request: ");
    scanf("%d", &n);
    printf("Enter the queue of disk positions to be read: ");
    for (i = 1; i <= n; i++) {
        scanf("%d", &temp);
        if (temp >= head) {
            queue1[temp1] = temp;
            temp1++;
        } else {
            queue2[temp2] = temp;
            temp2++;
        }
    }
